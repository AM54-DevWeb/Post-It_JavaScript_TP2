{
    "opts": {
    "template": "templates/clean-jsdoc-theme",
    "theme_opts": {"theme": "dark"} ,
    "encoding": "utf8", 
    "destination": "./doc/", 
    "recurse": true 
    },
    "source": {
    "include": ["./js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
    }
}